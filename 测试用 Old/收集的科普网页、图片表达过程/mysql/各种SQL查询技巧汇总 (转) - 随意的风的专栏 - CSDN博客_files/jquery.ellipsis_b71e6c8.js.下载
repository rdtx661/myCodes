define("jquery.ellipsis",["require","exports","module"],function(){!function(t){t.fn.ellipsis=function(l){var e={row:1,onlyFullWords:!1,"char":"...",callback:function(){},position:"tail"};return l=t.extend(e,l),this.each(function(){var e=t(this),i=e.text(),a=i,r=a.length,o=e.height();e.text("a");var c=parseFloat(e.css("lineHeight"),10),h=e.height(),s=c>h?c-h:0,n=s*(l.row-1)+h*l.row;if(n>=o)return e.text(i),void l.callback.call(this);var u=1,f=0,d=i.length;if("tail"===l.position){for(;d>u;)f=Math.ceil((u+d)/2),e.text(i.slice(0,f)+l["char"]),e.height()<=n?u=f:d=f-1;i=i.slice(0,u),l.onlyFullWords&&(i=i.replace(/[\u00AD\w\uac00-\ud7af]+$/,"")),i+=l["char"]}else if("middle"===l.position){for(var x=0;d>u;)f=Math.ceil((u+d)/2),x=Math.max(r-f,0),e.text(a.slice(0,Math.floor((r-x)/2))+l["char"]+a.slice(Math.floor((r+x)/2),r)),e.height()<=n?u=f:d=f-1;x=Math.max(r-u,0);var p=a.slice(0,Math.floor((r-x)/2)),M=a.slice(Math.floor((r+x)/2),r);l.onlyFullWords&&(p=p.replace(/[\u00AD\w\uac00-\ud7af]+$/,"")),i=p+l["char"]+M}e.text(i),l.callback.call(this)}),this}}(jQuery)});